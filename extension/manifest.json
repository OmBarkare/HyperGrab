{
    "name": "Check Downloads",
    "description": "DISCO",
    "version": "1",
    "manifest_version": 3,
    "permissions": [  "downloads","downloads.open","downloads.shelf", "webRequest"],
    "host_permissions": ["*://*/*"],
    "action": {
        "default_popup": "hello.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}